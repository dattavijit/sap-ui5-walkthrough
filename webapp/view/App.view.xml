						<mvc:XMLView viewName="sap.ui.demo.walkthrough.view.HelloPanel"></mvc:XMLView>
						<mvc:XMLView viewName="sap.ui.demo.walkthrough.view.InvoiceList"></mvc:XMLView>
					</content>
